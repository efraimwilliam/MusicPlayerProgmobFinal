<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuActivity"
        android:background="@drawable/menu">


    <ImageView
        android:id="@+id/musicplayer"
        android:layout_width="837px"
        android:layout_height="116px"
        android:layout_marginTop="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/musicplayer" />

    <ImageView
        android:id="@+id/account"
        android:layout_width="837px"
        android:layout_height="116px"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/musicplayer"
        app:srcCompat="@drawable/account" />

    <ImageView
        android:id="@+id/infoapps"
        android:layout_width="837px"
        android:layout_height="116px"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/account"
        app:srcCompat="@drawable/infoo" />

    <ImageView
        android:id="@+id/logoutacc"
        android:layout_width="837px"
        android:layout_height="116px"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="268dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoapps"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logoutt" />


</androidx.constraintlayout.widget.ConstraintLayout>